<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AMBSTORE" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">#F0A500</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#F0A500</item>
        <item name="colorSecondaryVariant">#F0A500</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.AMBSTORE.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="backch">
        <item name="android:background">#082032</item>
        <item name="srcCompat">@drawable/back</item>
    </style>

    <style name="textch">
        <item name="android:textColorHint">#EAEAEA</item>
        <item name="android:textColor">#EAEAEA</item>
    </style>

    <style name="Theme.AMBSTORE.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.AMBSTORE.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>